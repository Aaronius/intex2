package edu.byu.isys413.group1a.intex2.Misc;

/**
 * Signals that some type of data read/write error has occurred.
 *
 * @author Group 1A, isys@aaronhardy.com
 */
public class RenewalException extends Exception {
  
  /**
   * Creates a new instance of class
   */
  public RenewalException(String s) {
    super(s);
  }//constructor
  
  /**
   * Creates a new instance of class with an embedded exception
   */
  public RenewalException(String s, Throwable t) {
    super(s, t);
  }//constructor
  
}//class
